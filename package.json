{
  "private": true,
  "name": "searchfilm-client",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/server.js",
  "scripts": {
    "build": "npm run build:prod",
    "build:analize": "webpack --config webpack.analize.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "start": "node ./dist/server.js",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "storybook": "start-storybook -p 5001"
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    "> 0.5%",
    "IE 11"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/mocks/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/src/mocks/styleMock.js",
      "^@master(.*)$": "<rootDir>/src/@master$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^classes(.*)$": "<rootDir>/src/classes$1",
      "^helpers(.*)$": "<rootDir>/src/helpers$1",
      "^config(.*)$": "<rootDir>/src/config$1",
      "^actions(.*)$": "<rootDir>/src/actions$1",
      "^reducers(.*)$": "<rootDir>/src/reducers$1",
      "^selectors(.*)$": "<rootDir>/src/selectors$1",
      "^api(.*)$": "<rootDir>/src/api$1",
      "^mocks(.*)$": "<rootDir>/src/mocks$1",
      "^store(.*)$": "<rootDir>/src/store$1",
      "^tests(.*)$": "<rootDir>/src/tests$1",
      "^hooks(.*)$": "<rootDir>/src/hooks$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "css"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**"
    ],
    "roots": [
      "./src"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo [Husky] pre-commit"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-links": "6.4.22",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.4.22",
    "@storybook/manager-webpack5": "6.4.22",
    "@storybook/react": "6.4.22",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/user-event": "14.1.1",
    "babel-loader": "8.2.4",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.7.1",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "4.3.0",
    "jest": "27.5.1",
    "path": "0.12.7",
    "postcss": "8.4.12",
    "postcss-loader": "6.2.1",
    "postcss-preset-env": "7.4.3",
    "redux-devtools-extension": "2.13.9",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.8.1",
    "webpack-merge": "5.8.0",
    "webpack-visualizer-plugin": "0.1.11"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.8.1",
    "classnames": "2.3.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "express": "4.17.3",
    "final-form": "4.20.6",
    "history": "5.3.0",
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-final-form": "6.5.9",
    "react-redux": "8.0.0",
    "react-router-dom": "6.3.0",
    "redux": "4.1.2",
    "redux-dynamic-middlewares": "2.2.0",
    "redux-thunk": "2.4.1"
  }
}
